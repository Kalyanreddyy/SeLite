<?xml version="1.0" encoding="UTF-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
 xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>bootstrap@selite.googlecode.com</em:id>
    <em:name>SeLite Bootstrap</em:name>
    <em:version>0.77</em:version>
    <em:creator>Peter Kehl</em:creator>
    <em:description>It reloads your Selenium Core extension (JS) automatically, whenever changed. You don't need to restart Selenium IDE everytime you change a Core extension that you develop.
    </em:description>
    <em:type>2</em:type> <!--Type extension-->
    <em:homepageURL>http://code.google.com/p/selite/</em:homepageURL>
    <em:optionsURL>chrome://selenium-ide/content/optionsDialog.xul</em:optionsURL>
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id> <!-- Firefox -->
        <em:minVersion>20.*</em:minVersion>
        <em:maxVersion>26.*</em:maxVersion>
      </Description>
    </em:targetApplication>
  </Description>
</RDF>
